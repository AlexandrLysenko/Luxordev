{% if product.available %}
<form action="/cart/add" method="post" class="shappify_add_to_cart_form clearfix product_form monogram-form" data-product-id="{{ product.id }}" data-money-format="{{ shop.money_format }}" data-shop-currency="{{ shop.currency }}" id="product-form-{{ product.id }}" enctype="multipart/form-data">

  {% if settings.display_inventory_left %}
  <div class="items_left">
    {% if product.variants.first.inventory_management == "shopify" and product.variants.first.inventory_quantity > 0 %}
    {% capture items_left_text %}
    {% if product.variants.first.inventory_quantity == 1 %}
    {{ 'products.product.items_left_count.one' | t }}
    {% else %}
    {{ 'products.product.items_left_count.other' | t }}
    {% endif %}
    {% endcapture %}

    <p><em>{{ product.variants.first.inventory_quantity }} {{ items_left_text }}</em></p>
    {% endif %}
  </div>
  {% endif %}

  {% if product.options.size > 1 %}
  <div class="select">
    <select id="product-select-{{ product.id }}{{ product-form }}" name="id" class="multi_select">
      {% for variant in product.variants %}
      <option {% if variant == product.selected_or_first_available_variant %}selected="selected"{% endif %} value="{{ variant.id }}" data-sku="{{ variant.sku }}">{{ variant.title }}</option>
      {% endfor %}
    </select>
  </div>
  {% elsif product.options.size == 1 %}
  {% if product.variants.size > 1 or product.options[0] != "Title" %}
  <div class="select">
    <label>{{ product.options[0] }}</label>
    <select id="product-select-{{ product.id }}{{ product-form }}" name="id">
      {% for variant in product.variants %}
      <option {% if variant == product.selected_or_first_available_variant %}selected="selected"{% endif %} value="{{ variant.id }}" data-sku="{{ variant.sku }}">{{ variant.title }}</option>
      {% endfor %}
    </select>
  </div>
  {% else %}
  <input type="hidden" name="id" value="{{ product.variants.first.id }}" />
  {% endif %}
  {% else %}
  <input type="hidden" name="id" value="{{ product.variants.first.id }}" />
  {% endif %}
  <span class="mono_hidden_pro">
	<input type="hidden" id="monogram" value="{{ product.id }}" onClick="show_review()">
  </span>
  {% if settings.product_form_style == "swatches" %}
  {% if product.variants.size > 1 or product.options[0] != "Title" %}
  <div class="swatch_options">
    {% for option in product.options %}
    {% include 'product-swatch' with option %}
    {% endfor %}
  </div>
  {% endif %}
  {% endif %}
  
<div id="infiniteoptions-container"></div>
  
  {% if settings.display_product_quantity %}
  <div class="left">
    <label for="quantity">{{ 'products.product.quantity' | t }}</label>
    <input type="number" min="1" size="2" class="quantity" name="quantity" id="quantity" value="1" {% if product.variants.first.inventory_management == 'shopify' and product.variants.first.inventory_quantity > 0 and product.variants.first.inventory_policy == "deny" %}max="{{ product.variants.first.inventory_quantity }}"{% endif %} />
  </div>
  {% endif %}
  <div class="purchase clearfix {% if settings.display_product_quantity %}inline_purchase{% endif %}">
    {% if settings.cart_action == 'reload_page' %}
    <input type="hidden" name="return_to" value="back" />
    {% elsif settings.cart_action == 'redirect_checkout' %}
    <input type="hidden" name="return_to" value="/checkout" />
    {% endif %}

    {% capture add_to_cart_label %}{% if collection_handles contains 'pre-order' %}{{ 'collections.general.pre_order' | t }}{% else %}{{ 'products.product.add_to_cart' | t }}{% endif %}{% endcapture %}
    
   <div class='ba-vol-wrapper'></div>
    
    <div class="add-mon-outter clearfix">
      <div class="option_title">Personalize It</div>
      <div class="check-outter">
        <div class="mon-price checkbox-style">
          <input type="checkbox" id="price-chk" onchange="fnTest(this);"/>
          {% assign monogram_price = product.metafields.hidden_product_price %}
          <label for="price-chk"><table class="Mono"><tr><td><img src="https://cdn.shopify.com/s/files/1/1195/2834/files/Monogram-Image.jpg?1079566102406019721" style="width:75px;height:75px;"></td><td>Add Custom Monogramming? <span> ${{ monogram_price.hidden_product_price }}</span></td></tr></table></label>
        </div>
        <!--<a style="display:none;" href="javascript:void(0)" class="mono-show" onclick="show_sinup()"> SELECT TO CUSTOMIZE </a>-->
      </div>
    
 

      <div class="preview-wrp" style="display:none;">
          <label for="price-chk">Monogram: <span> ${{ monogram_price.hidden_product_price }}</span></label>
          <div class="preview-block">
          	<div class="modified-monogram preview-outter">
              <div class="main-preview-inner"></div>
            </div>
          </div>
        </div>
      
    </div>
    
    <div>
      <div class="mono-type">
      <input type="hidden" id="monogram_text" >
      </div>
       
       <input type="hidden" id="style" >
       <input type="hidden" id="color" >
      
             
    </div>

    
    
    <div class="quantity-wrp clearfix"> 
      <div class="quantity-outter clearfix">  
        <label for="quantity">Qty </label>
        <input min="1" type="number" id="quantity" name="quantity" value="1" />
       </div>
	  <button id="ToCart" type="submit" name="add" class="hide action_button add_to_cart" data-label={{ add_to_cart_label | json }}><span class="text">{{ add_to_cart_label }}</span></button>
      <span class="outer_butt_border cart-button"> <a href="javascript:void(0)" class="action_button right button button--winona button--border-thin button--text-thick button--inverted add_to_cart1" data-text="{{ add_to_cart_label }}"><span class="text">{{ add_to_cart_label }}</span></a></span>
         <!-- Bold: Options 4-2 -->
{% include 'shappify-options' %}
<!-- // end Options 4-2 -->
     </div> 

  </div>
 
  <div><span class="scolumn"><img src="{{'custom_text_icon.png' | asset_url }}" alt="Icon" /></span><span class="three column"><span class="custom_text_style" style="text-align:center">{{settings.custom_text}}</span></span></div>
	<div class="saso-volumes"></div>
<div class="saso-bundle"></div>
  <div class="powr-countdown-timer" id="02a2ac48_1507064787"> </div>
  
</form>




<div class="monogram-popup mfp-hide" id="monogram-popup" onClick="show_review()">
  <div class="popup-header clearfix">
    <h5>Customize your monogram</h5>
    <a href="javascript:void(0)" class="mfp-close" title="Close (Esc)">Ã—</a>
  </div>
  <div class="clearfix monogram-content">
    <div class="monotype-outter monotype-monogram">
      <div class="heading-outter">
        <h6>step <span>1</span><p style="float: right; width: 66%;">Choose Font</p></h6>
      </div>
      <div class="monotype-style">
        {% include 'product-monogram-type' %}
      </div>
    </div>
    <div class="monotype-outter monotype-color">
      <div class="heading-outter">
        <h6>step <span>2</span><p style="float: right; width: 66%;">choose a color</p></h6>        
      </div>
      <div class="monotype-style">
        {% include 'product-monogram-color' %}
      </div>
    </div>
    <div class="monotype-preview">
      <div class="heading-outter">
        <h6>step <span>3</span><p style="float: right; width: 72%; font-size: 14px;"><span></span></p></h6>        
      </div>
      <div class="form-outter">
        <label>
          <p class="error"></p>
          <script>
            /*alert($(this).attr('data-error'));*/       
            $(document).ready(function(){
            var firsttext = $('#Conv_SalinasDemo-Regular').attr('data-error');
            $('#monogram').attr('placeholder',firsttext);
            });
          </script>
          <input type="text" class="form-style" name="monogram" id="monogram">
        </label>
        <div class="preview-outter">
          {% assign instructions = product.metafields.monogram_image %}
          {% assign arealeft = product.metafields.monogram_arealeft%}
          {% assign areatop = product.metafields.monogram_areatop%}
          
          {% assign areawidth = product.metafields.monogram_areawidth%}
          {% assign areaheight = product.metafields.monogram_areaheight%}
          
          <div class="preview-inner" {%comment%}style="background-image: url('http://shopifyapp.metizcloud.com/luxorlinens/uploads/monogram_upload_image/{{ instructions.monogram_image }}'); background-repeat: no-repeat; background-size: 100% 100%;"{%endcomment%}>
			
            
          </div>
        </div>
        <input type="submit" class="btn btn-preview" value="Preview" onClick="show_review()">
        <input type="submit" class="btn btn-confirm" value="Confirm" onClick="show_confirm()">
        <p>{{settings.mono_confirmation}}</p>
      </div>
    </div>
  </div>
  {% assign hidden_product_id = product.metafields.hidden_product_id%}
  <input type="hidden" id="hidden_product" value="{{hidden_product_id.hidden_product_id}}">
  <span id="hidden_pro"></span>
</div>

